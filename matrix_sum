#include<stdio.h>
#include<conio.h>
int main()
{
	int a[2][2],b[2][2],sum[2][2],i,j;
 printf("\nEnter elements of 1st matrix:\n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("Enter element a%d%d ",i+1,j+1);
			scanf("%d",&a[i][j]);
		}
	}
	
	printf("\nEnter elements of 2nd matrix:\n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			printf("Enter element b%d%d ",i+1,j+1);
			scanf("%d",&b[i][j]);
		}
	}
	//adding two matrices
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
			sum[i][j]=a[i][j]+b[i][j];
		}
		
	}
	//printing result 
	printf("Sum of two matrices is:\n");
	for(i=0;i<2;i++)
	{
		for(j=0;j<2;j++)
		{
		printf("\t%d",sum[i][j]);	
	
		}
			printf("\n");
	}
	return 0;
}