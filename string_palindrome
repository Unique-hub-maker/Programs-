#include<stdio.h>
#include<string.h>
char palindrome(char );
int main()
{
	char a[10],b[10];
	int c;
	printf("Enter string :-");
	scanf("%s",a);
     	c=strcmp(a,b);
	if (c==0)
	{
		printf("%s is palindrome :\n",a);
		
	}
	else
	{
			printf("%s is not  palindrome :\n",a);
	}
	return 0;
}
char palindrome(char a)
{  
char b[10],a[10];
	int c,l,j=0,i;
		l=strlen(a)-1;
	for(i=l;i>=0;i--)
	{
		b[j]=a[i];
		j=j+1;
		
	}
	return c;
}